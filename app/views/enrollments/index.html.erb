<h1>Enrollments</h1>
<%= link_to "Add Enrollment", new_course_enrollment_path(@course) %>

<h1>Teachers: </h1>
<ul>
  <% @teachers.each do |teacher| %>
      <li><%= teacher.user.full_name %></li>
  <% end %>
</ul>
<h1>T.A.s: </h1>
<ul>
  <% @tas.each do |tas| %>
      <li><%= tas.user.full_name %></li>
  <% end %>
</ul><h1>Students: </h1>
<ul>
  <% @students.each do |student| %>
      <li><%= student.user.full_name %></li>
  <% end %>
</ul>